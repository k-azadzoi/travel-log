{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "PORT": "1337",
        "DATABASE_URL":"@travel-log-db",
        "CORS_ORIGIN": "https://build-umber.vercel.app",
        "API_KEY": "@travel-log-api-key"
    }
}